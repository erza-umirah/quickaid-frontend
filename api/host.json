{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5
      }
    },
    "logLevel": {
      "Function": "Information",
      "Host.Results": "Information",
      "default": "Information"
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api",
      "maxOutstandingRequests": 200,
      "maxConcurrentRequests": 100,
       "customHeaders": {
        "Access-Control-Allow-Origin": "http://localhost"
      }
    },
    "timers": {
      "scheduleMonitor": {
        "enabled": true,
        "interval": "00:05:00",
        "runOnStartup": false
      }
    }
  },
  "functionTimeout": "00:05:00"
}
